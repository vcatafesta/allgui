      FONTITALIC .F.
      FONTUNDERLINE .F.
      FONTSTRIKEOUT .F.
      HELPID Nil
      VISIBLE .T.
      TRANSPARENT .T.
      ACTION Nil
      AUTOSIZE .F.
      BACKCOLOR NIL
      FONTCOLOR NIL
   END LABEL

   DEFINE IMAGE Image_1
      ROW    180
      COL    350
      WIDTH  172
      HEIGHT 110
      PICTURE "SuperSDC"
      HELPID Nil
      VISIBLE .T.
      STRETCH .T.
      ACTION Nil
   END IMAGE

END WINDOW
